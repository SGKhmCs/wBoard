        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="board"
                                 constraintName="fk_board_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
